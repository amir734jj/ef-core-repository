<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Description>EntityFramework.Core repository pattern</Description>
        <RepositoryUrl>https://github.com/amir734jj/ef-core-repository</RepositoryUrl>
        <PackageProjectUrl>https://github.com/amir734jj/ef-core-repository</PackageProjectUrl>
        <AssemblyVersion>3.1.15</AssemblyVersion>
        <PackageReleaseNotes>Fix duplicate analyzing of reflection data by making them singleton</PackageReleaseNotes>
        <Version>3.1.15</Version>
        <RootNamespace>EfCoreRepository</RootNamespace>
        <AssemblyName>SimpleEfCoreRepository</AssemblyName>
        <LangVersion>latest</LangVersion>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="AgileObjects.AgileMapper" Version="1.8.1" />
      <PackageReference Include="InfoViaLinq" Version="3.0.5" />
      <PackageReference Include="Microsoft.EntityFrameworkCore" Version="6.0.36" />
      <PackageReference Include="Microsoft.EntityFrameworkCore.Relational" Version="6.0.36" />
    </ItemGroup>

</Project>
