<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.1</TargetFramework>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Description>EntityFramework.Core repository pattern</Description>
        <RepositoryUrl>https://github.com/amir734jj/ef-core-repository</RepositoryUrl>
        <PackageProjectUrl>https://github.com/amir734jj/ef-core-repository</PackageProjectUrl>
        <AssemblyVersion>3.0.3</AssemblyVersion>
        <PackageReleaseNotes>Used a built in function to find the ID.</PackageReleaseNotes>
        <Version>3.0.3</Version>
        <RootNamespace>EfCoreRepository</RootNamespace>
        <AssemblyName>SimpleEfCoreRepository</AssemblyName>
        <PackageReleaseNotes>Added auto mapping functionality.</PackageReleaseNotes>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Microsoft.EntityFrameworkCore" Version="3.1.0" />
    </ItemGroup>

</Project>
