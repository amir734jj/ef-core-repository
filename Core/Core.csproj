<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net8.0</TargetFramework>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Description>EntityFramework.Core repository pattern</Description>
        <RepositoryUrl>https://github.com/amir734jj/ef-core-repository</RepositoryUrl>
        <PackageProjectUrl>https://github.com/amir734jj/ef-core-repository</PackageProjectUrl>
        <AssemblyVersion>3.1.16</AssemblyVersion>
        <PackageReleaseNotes>Added a feature to check whether entity has open references, this can be used to check if entity can be deleted or not</PackageReleaseNotes>
        <Version>3.1.16</Version>
        <RootNamespace>EfCoreRepository</RootNamespace>
        <AssemblyName>SimpleEfCoreRepository</AssemblyName>
        <LangVersion>latest</LangVersion>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="AgileObjects.AgileMapper" Version="1.8.1" />
      <PackageReference Include="InfoViaLinq" Version="3.0.5" />
      <PackageReference Include="Microsoft.EntityFrameworkCore" Version="8.0.19" />
      <PackageReference Include="Microsoft.EntityFrameworkCore.Relational" Version="8.0.19" />
    </ItemGroup>

</Project>
